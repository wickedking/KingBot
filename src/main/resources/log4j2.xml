<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="error" name="KingBot" packages="">
  <Appenders>
    <File name="KingBot" fileName="logs/KingBot.log">
      <PatternLayout>
        <Pattern>%d %p %c{1.} [%t] %m%n</Pattern>
      </PatternLayout>
    </File>
  </Appenders>
  <Loggers>
    <Root level="error">
      <AppenderRef ref="KingBot"/>
    </Root>
  </Loggers>
</Configuration>
